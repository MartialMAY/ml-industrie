{"ast":null,"code":"import Navbar from \"../components/Navbar.vue\";\nimport axios from \"axios\";\nimport store from \"../modules/store.json\";\nexport default {\n  components: {\n    Navbar\n  },\n\n  data() {\n    return {\n      user: {\n        name: \"\",\n        email: \"\",\n        phone: \"\",\n        password: \"\"\n      },\n      loader: false\n    };\n  },\n\n  methods: {\n    registerUser() {\n      this.loader = true;\n      axios.post(store.api_host + \"/user\", {\n        name: this.user.name,\n        email: this.user.email,\n        password: this.user.password,\n        phone: this.user.phone\n      }).then(response => {\n        if (response.status === 200 || response.status === 201) {\n          return response;\n        } else {\n          console.log(\"Erreur d'envoi de formulaire\");\n        }\n      }).then(response => {\n        console.log(response); //Envoi vers la page utilisateur connecté\n      }).catch(function (error) {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8FA,OAAOA,MAAP,MAAmB,0BAAnB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,uBAAlB;AAEA,eAAe;EACbC,UAAU,EAAE;IAAEH;EAAF,CADC;;EAEbI,IAAI,GAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,EADF;QAEJC,KAAK,EAAE,EAFH;QAGJC,KAAK,EAAE,EAHH;QAIJC,QAAQ,EAAE;MAJN,CADD;MAOLC,MAAM,EAAE;IAPH,CAAP;EASD,CAZY;;EAcbC,OAAO,EAAE;IACPC,YAAY,GAAG;MACb,KAAKF,MAAL,GAAc,IAAd;MAEAT,KAAI,CACDY,IADH,CACQX,KAAK,CAACY,QAAN,GAAiB,OADzB,EACkC;QAC9BR,IAAI,EAAE,KAAKD,IAAL,CAAUC,IADc;QAE9BC,KAAK,EAAE,KAAKF,IAAL,CAAUE,KAFa;QAG9BE,QAAQ,EAAE,KAAKJ,IAAL,CAAUI,QAHU;QAI9BD,KAAK,EAAE,KAAKH,IAAL,CAAUG;MAJa,CADlC,EAOGO,IAPH,CAOSC,QAAD,IAAc;QAClB,IAAIA,QAAQ,CAACC,MAAT,KAAoB,GAApB,IAA2BD,QAAQ,CAACC,MAAT,KAAoB,GAAnD,EAAwD;UACtD,OAAOD,QAAP;QACF,CAFA,MAEO;UACLE,OAAO,CAACC,GAAR,CAAY,8BAAZ;QACF;MACD,CAbH,EAcGJ,IAdH,CAcSC,QAAD,IAAc;QAClBE,OAAO,CAACC,GAAR,CAAYH,QAAZ,EADkB,CAElB;MACD,CAjBH,EAkBGI,KAlBH,CAkBS,UAAUC,KAAV,EAAiB;QACtBH,OAAO,CAACC,GAAR,CAAYE,KAAZ;MACD,CApBH;IAqBD